{
  "connection": "default",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "tcNo": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "name": {
      "required": true,
      "type": "string"
    },
    "surname": {
      "required": true,
      "type": "string"
    },
    "phone": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "birthDate": {
      "required": true,
      "type": "date"
    },
    "balance": {
      "type": "float",
      "default": "0",
      "required": true
    }
  }
}